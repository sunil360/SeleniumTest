
class A
    {
        public void display()
        {
           System.out.println("Display Method");
        }
    }

    class B extends A
    {
        public void display()
        {
            System.out.println("Show Method");
        }
    }

    class Program
    {
        public static void main(String[] args)
        {
            A a = new B();
             
        
            
        }
    }